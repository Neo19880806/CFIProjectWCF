<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2"/>
    </startup>
    <system.serviceModel>
        <bindings>
            <webHttpBinding>
                <binding name="webHttpBindingDefault">
                    <security mode="Transport" />
                </binding>
            </webHttpBinding>
        </bindings>
        <serviceHostingEnvironment aspNetCompatibilityEnabled="true" />
        <behaviors>
            <endpointBehaviors>
                <behavior name="webHttpBindingDefault">
                    <webHttp />
                </behavior>
            </endpointBehaviors>
            <serviceBehaviors>
                <behavior name="Default">
                    <serviceMetadata httpGetEnabled="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service behaviorConfiguration="Default" name="CFIWCFServiceLiabrary.CFIDBService">
                <endpoint address="MybasicHttpBinding" binding="basicHttpBinding"
                    bindingConfiguration="" name="MybasicHttpBinding" contract="CFIWCFServiceLiabrary.ICFIDBService" />
                <endpoint address="MywsHttpBinding" binding="wsHttpBinding" bindingConfiguration=""
                    name="wsHttpBinding" contract="CFIWCFServiceLiabrary.ICFIDBService" />
                <endpoint address="MywebHttpBinding" behaviorConfiguration="webHttpBindingDefault"
                    binding="webHttpBinding" bindingConfiguration="" name="MywebHttpBinding"
                    contract="CFIWCFServiceLiabrary.ICFIDBService" />
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:1988/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>
